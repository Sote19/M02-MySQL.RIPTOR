import random
from datetime import datetime

# Generar el INSERT con los 5000 registros
insert = "INSERT INTO Devolucion (fecha_dev, motivo_dev, id_prod, id_vent) VALUES\n"

for i in range(80):
    fecha_dev = datetime(random.randint(2000, 2024), random.randint(1, 12), random.randint(1, 28),
                     random.randint(0, 23), random.randint(0, 59), random.randint(0, 59)).strftime('%Y-%m-%d %H:%M:%S')
    motivo_dev = "Motivo" + str(random.randint(1, 100))  # Motivo aleatorio
    id_prod = random.randint(1, 10000)  # Asumiendo que tienes 100 productos en tu tabla de productos
    id_venta = random.randint(127, 327)  # Asumiendo que tienes 5000 ventas en tu tabla de ventas
    
    # Agregar los valores al INSERT
    insert += f"('{fecha_dev}', '{motivo_dev}', {id_prod}, {id_venta}),\n"

# Eliminar la coma extra al final y agregar punto y coma
insert = insert[:-2] + ";"

# Imprimir el INSERT generado
print(insert)
